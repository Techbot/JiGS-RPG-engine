{#
/**
 * @file
 * Theme override for a breadcrumb trail.
 *
 * Available variables:
 * - breadcrumb: Breadcrumb trail items.
 */
#}
{% if breadcrumb %}
  {%
    include "@eclecticmeme/navigation/breadcrumb/breadcrumb.twig" with {
      modifier_class: '',
      heading: 'Breadcrumb'|t,
      breadcrumb: breadcrumb
    } only
  %}
{% endif %}
